{
  "name": "interface-starter",
  "version": "0.1.0",
  "description": "Starter Jade, Stylus, ES6 project template.",
  "private": "true",
  "main": "server.js",
  "scripts": {
    "build-css": "gulp build-css",
    "build-js": "gulp build-js",
    "build-html": "gulp build-html",
    "watch-files": "gulp watch",
    "clean": "rm -rf static/css && mkdir -p static/css && rm -rf static/js && rm -rf static/*.html && mkdir -p static/js",
    "build": "npm run clean && npm run build-css && npm run build-js && npm run build-html",
    "watch": "npm run build && npm run watch-files & nodemon server -e js,jade",
    "start": "node server"
  },
  "author": "The Flaming Dragon",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-jade": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.3.1",
    "jade": "^1.11.0",
    "jeet": "<=6.1.2",
    "morgan": "^1.7.0",
    "nib": "^1.1.0",
    "nodemon": "^1.9.1",
    "rupture": "^0.6.1",
    "stylus": "^0.53.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "gulp-babel": "^6.1.2",
    "gulp-jade": "^1.1.0",
    "gulp-livereload": "^3.8.1",
    "gulp-minify": "0.0.12",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.4"
  },
  "babel": {
    "presets": ["es2015"]
  }
}
